<%= form_for(@reservation) do |f| %>
  <%= f.label "start date" %>
  <%= date_select(:reservation, :start_date) %><br>

  <%= f.label "end date" %>
  <%= date_select(:reservation, :end_date) %>

  <%= f.hidden_field :guest_id, :value => current_user.id %>
  <%= f.hidden_field :listing_id, :value => params[:id]%>
  <%= f.submit "New Reservation" %>
<% end %>
