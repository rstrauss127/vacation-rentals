<section class="todoapp">
  <header class="header">
    <h1>Past Reservations</h1>
  </header>
  <section class="main">
    <ul class="todo-list">
      <% @reservations.each do |res| %>
        <li>
          <div class="view">
            <label>Guest: <%= res.guest_email%></label>
            <label>Stayed at: <%= res.listing_title %></label>
            <label>On: <%= res.start_date.to_date %></label>
          </div>
        </li>
      <% end %>
    </ul>
  </section>
</section>
