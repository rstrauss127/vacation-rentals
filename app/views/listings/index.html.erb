<section class="todoapp">

  <header class="header">
		<h1>Listings</h1>
    <%= link_to "New Listing", new_listing_path, class: "new-todo" %>
	</header>

  <section class="main">
    <ul class="todo-list">
      <% @listings.each do |list| %>
        <li>
          <div class="view">
            <label><a href="#"><%= link_to list.title, listing_path(list) %></a></label>
          </div>
        </li>
    <% end %>
    </ul>
  </section>
</section>

<%= render "partials/footer" %>
